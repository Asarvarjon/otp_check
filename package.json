{
  "name": "otp_check",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "ts-node ./index.ts",
    "seed:run": "knex seed:run --knexfile ./src/database/knexfile.ts",
    "seed:make": "knex seed:make --knexfile ./src/database/knexfile.ts",
    "migrate:latest": "env-cmd knex migrate:latest --knexfile ./src/database/knexfile.ts",
    "migrate:make": "env-cmd knex migrate:make --migrations-directory ./src/database/migrations -x ts",
    "migrate:rollback": "env-cmd knex migrate:rollback --knexfile ./src/database/knexfile.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "env-cmd": "^10.1.0",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "knex": "^2.2.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.14",
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
